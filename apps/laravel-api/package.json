{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "laravel-api",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "./vendor/bin/sail up",
    "build": "composer install --no-dev --optimize-autoloader && php artisan migrate --force && php artisan config:cache",
    "up": "./vendor/bin/sail up -d",
    "down": "./vendor/bin/sail down",
    "test": "./vendor/bin/sail artisan test"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "axios": "^1.12.2",
    "concurrently": "^9.2.1",
    "laravel-vite-plugin": "^2.0.1",
    "tailwindcss": "^4.1.14",
    "vite": "^7.1.10"
  }
}
